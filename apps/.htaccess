RewriteEngine On
Order Deny,Allow

<Files settings.pcef>
Deny from all
</Files>

<Files waypoints.php>
Deny from all
</Files>

#Rewrite rules
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-l

RewriteRule ^(.+)$ index.php


AddDefaultCharset utf-8